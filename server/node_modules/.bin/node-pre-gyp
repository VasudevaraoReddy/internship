#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/theia/loginsample/server/node_modules/.pnpm/@mapbox/node-pre-gyp@1.0.6/node_modules/@mapbox/node-pre-gyp/bin/node-pre-gyp/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/@mapbox/node-pre-gyp@1.0.6/node_modules/@mapbox/node-pre-gyp/bin/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/@mapbox/node-pre-gyp@1.0.6/node_modules/@mapbox/node-pre-gyp/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/@mapbox/node-pre-gyp@1.0.6/node_modules/@mapbox/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/@mapbox/node-pre-gyp@1.0.6/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/@mapbox/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/node_modules:/home/theia/loginsample/server/node_modules:/home/theia/loginsample/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/loginsample/server/node_modules/@mapbox/node-pre-gyp/bin/node-pre-gyp/node_modules:/home/theia/loginsample/server/node_modules/@mapbox/node-pre-gyp/bin/node_modules:/home/theia/loginsample/server/node_modules/@mapbox/node-pre-gyp/node_modules:/home/theia/loginsample/server/node_modules/@mapbox/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@mapbox/node-pre-gyp/bin/node-pre-gyp" "$@"
else 
  exec node  "$basedir/../@mapbox/node-pre-gyp/bin/node-pre-gyp" "$@"
fi
