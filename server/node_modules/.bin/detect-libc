#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/theia/loginsample/server/node_modules/.pnpm/detect-libc@1.0.3/node_modules/detect-libc/bin/detect-libc.js/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/detect-libc@1.0.3/node_modules/detect-libc/bin/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/detect-libc@1.0.3/node_modules/detect-libc/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/detect-libc@1.0.3/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/node_modules:/home/theia/loginsample/server/node_modules:/home/theia/loginsample/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/loginsample/server/node_modules/detect-libc/bin/detect-libc.js/node_modules:/home/theia/loginsample/server/node_modules/detect-libc/bin/node_modules:/home/theia/loginsample/server/node_modules/detect-libc/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../detect-libc/bin/detect-libc.js" "$@"
else 
  exec node  "$basedir/../detect-libc/bin/detect-libc.js" "$@"
fi
