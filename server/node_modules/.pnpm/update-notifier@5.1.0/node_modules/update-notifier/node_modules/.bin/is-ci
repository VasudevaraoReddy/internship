#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/theia/loginsample/server/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/bin.js/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/is-ci@2.0.0/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/node_modules:/home/theia/loginsample/server/node_modules:/home/theia/loginsample/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/update-notifier@5.1.0/node_modules/update-notifier/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/update-notifier@5.1.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../is-ci@2.0.0/node_modules/is-ci/bin.js" "$@"
else 
  exec node  "$basedir/../../../../../is-ci@2.0.0/node_modules/is-ci/bin.js" "$@"
fi
