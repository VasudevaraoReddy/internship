#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/theia/loginsample/server/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/bin/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/mkdirp@1.0.4/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/node_modules:/home/theia/loginsample/server/node_modules:/home/theia/loginsample/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/tar@6.1.11/node_modules/tar/node_modules:/home/theia/loginsample/server/node_modules/.pnpm/tar@6.1.11/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js" "$@"
else 
  exec node  "$basedir/../../../../../mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js" "$@"
fi
